eigrp 4 routers

en
conf t
hostname R1
int f0/0
ip add 192.168.1.1 255.255.255.0
no sh
exit
int s0/0
ip add 172.10.0.1 255.255.0.0
no sh
exit
int s0/1
ip add 169.0.0.1 255.255.0.0
no sh
exit
router eigrp 10
no auto-summary
net 172.10.0.0
net 169.0.0.0
net 192.168.1.0
end
